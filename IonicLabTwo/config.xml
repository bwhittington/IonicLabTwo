<?xml version='1.0' encoding='utf-8'?>
<widget defaultlocale="en-US" id="hmb.gradacademy.labtwo" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:vs="http://schemas.microsoft.com/appx/2014/htmlapps">
    <name>IonicLabTwo</name>
    <description>A blank project that uses Apache Cordova to help you build an app that targets multiple mobile platforms: Android, iOS, and the Windows Universal Platform.</description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">Your name here</author>
    <vs:template-name>BlankTS</vs:template-name>
    <vs:toolsetVersion>6.5.0</vs:toolsetVersion>
    <content src="index.html" />
    <access origin="*" />
    <preference name="SplashScreen" value="screen" />
    <preference name="windows-target-version" value="10.0" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^5.2.1" />
    <engine name="ios" spec="4.2.0" />
    <engine name="windows" spec="4.4.2" />
    <plugin name="cordova-plugin-whitelist" spec="^1.2.2" />
</widget>
